{
    "name": "d3yii2/d3printeripp",
    "description": "IPP Printer Management Component for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2", "ipp", "printer", "cups", "printing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lauris Mierkalns",
            "email": "404dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "~2.0.14",
        "obray/ipp": "*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "app\\components\\printer\\": "src/printer/",
            "app\\components\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "app\\tests\\": "tests/"
        }
    },
    "extra": {
        "yii2-extension": {
            "name": "ipp-printer-manager",
            "version": "1.0.0"
        }
    }
}
